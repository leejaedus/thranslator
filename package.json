  "bin": {
    "electron": "./node_modules/.bin/electron",
    "flow": "./node_modules/.bin/flow"
  },
  "scripts": {
    "hot-server": "cross-env NODE_ENV=development node -r babel-register ./dev/server.development.js",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./dev/main.development",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  }
